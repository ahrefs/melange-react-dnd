{
	"name": "@ahrefs/bs-react-dnd",
	"version": "1.0.2",
	"description": "Reason bindings for react-dnd",
	"author": "ahrefs",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ahrefs/bs-react-dnd.git"
	},
	"homepage": "https://github.com/ahrefs/bs-react-dnd#readme",
	"bugs": {
		"url": "https://github.com/ahrefs/bs-react-dnd/issues"
	},
	"keywords": [
		"react",
		"reason-react",
		"reason",
		"reasonml",
		"bucklescript",
		"drag-and-drop",
		"dnd"
	],
	"scripts": {
		"prestart": "yarn install && bsb -clean-world",
		"start": "yarn bs:watch",
		"start:reshowcase": "reshowcase start --entry=examples/sortable/index.bs.js --port=9001 --template=examples/sortable/index.html",
		"bs:build": "bsb -make-world",
		"bs:watch": "bsb -make-world -clean-world -w",
		"bs:setup": "bsb -make-world"
	},
	"dependencies": {
		"react-dnd": "^14.0.3",
		"react-dnd-html5-backend": "^14.0.1"
	},
	"devDependencies": {
		"bs-platform": "^9.0.2",
		"bs-webapi": "^0.19.1",
		"rationale": "^0.1.3",
		"reshowcase": "^4.4.0"
	},
	"peerDependencies": {
		"react": "^16.8.1 || ^17.0.0",
		"react-dom": "^16.8.1 || ^17.0.0",
		"reason-react": "^0.9.1"
	}
}
